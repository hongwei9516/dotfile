                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "right_arrow",
                        "modifiers": {
                            "mandatory": [
                                "left_command",
                                "left_option",
                                "left_control",
                                "left_shift"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "apple_top_case_display_brightness_increment"
